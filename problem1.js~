var multiples = [];
var count, i;

for (i = 0; i == 1000; i++) {
    if (i % 3 === 0 || i % 5 === 0) {
        multiples.push(i);
        count++;
    }
    console.log("loop is running");
}

for (var i = 0; i < 10; i++) {
    console.log(i);
}

console.log("Ricardo");
console.log(multiples.length);